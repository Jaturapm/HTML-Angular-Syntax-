{
    "name": "vscode-angular-syntax",
    "displayName": "vscode-angular-syntax",
    "description": "Syntax highlighting for angular-html files.",
    "version": "0.0.1",
    "publisher": "dunstontc",
    "repository": {
        "type": "git",
        "url": "https://github.com/dunstontc/vscode-angular-syntax.git"
    },
    "bugs": {"url": "https://github.com/dunstontc/vscode-angular-syntax/issues"},
    "scripts": {"start": "gulp watch"},
    "engines": {"vscode": "^1.20.0"},
    "categories": ["Programming Languages"],
    "contributes": {
        "languages": [
            {
                "id": "angular-html",
                "aliases": ["Angular-html"],
                "extensions": [".component.html"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Angular-html",
                "scopeName": "angular-html",
                "path": "./syntaxes/angular-html.tmLanguage.json",
                "embeddedLanguages": {
                  "text.html.basic": "html",
                  "text.html.angular-html": "angular-html",
                  "text.pug": "pug",
                  "text.haml": "haml",
                  "source.css": "css",
                  "source.css.scss": "scss",
                  "source.css.less": "less",
                  "source.css.postcss": "postcss",
                  "source.sass": "sass",
                  "source.stylus": "stylus",
                  "source.js": "javascript",
                  "source.ts": "typescript",
                  "source.coffee": "coffeescript",
                  "text.html.markdown": "md",
                  "source.yaml": "yaml",
                  "source.json": "json",
                  "source.php": "php"
                }
            }
        ]
    },
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-json5-to-json": "^0.2.1",
        "gulp-merge-json": "^1.3.0"
    }
}
